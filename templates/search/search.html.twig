{% extends 'base.html.twig' %}

{% block title %}{% endblock %}

{% block body %}
	<main class="container ">
		<section class="row">
			<table class="table table-bordered table-striped mt-4">
				<thead>
					<tr>
						<th>id</th>
						<th>name</th>
						<th>email</th>
					</tr>
				</thead>
				<tbody>

					{% for row in result %}

						<tr>
							<td>{{row.id}}</td>
							<td>{{row.name}}</td>
							<td>{{row.email}}></td>
						</tr>
					{% endfor %}
				</tbody>
			</table>
		</section>
	</main>
{% endblock %}
